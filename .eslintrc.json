{
    "env":{
        "es2022":true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "unicorn"
    ],
    "extends":"plugin:unicorn/all",
    "rules": {
        "unicorn/filename-case": [
            "error",
            {
                "cases": {
                    "camelCase": true,
                    "pascalCase": true
                }
            }
        ]
    }
    }
